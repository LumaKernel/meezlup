{
  "name": "meetzup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "npm run _lint:eslint && npm run _lint:prettier",
    "_lint:eslint": "eslint \"src/**/*.{ts,tsx,mjs}\" \"*.{ts,mjs}\"",
    "_lint:prettier": "prettier . --list-different",
    "lint-fix": "npm run lint -- --fix && prettier . --write",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prisma:setup-env": "node scripts/prisma-env.js",
    "prisma:generate": "npm run prisma:setup-env && prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:prod": "npm run prisma:setup-env && prisma migrate deploy",
    "prisma:migrate": "node scripts/migrate.js",
    "prisma:studio": "prisma studio",
    "db:up": "docker-compose up -d postgres",
    "db:down": "docker-compose down",
    "db:migrate:test": "DATABASE_URL=\"postgresql://meetzup:meetzup_dev@localhost:5432/meetzup\" npm run prisma:migrate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.7.0",
    "@effect/platform": "^0.87.5",
    "@effect/platform-node": "^0.88.9",
    "@heroui/react": "^2.7.11",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/adapter-neon": "^6.11.1",
    "@prisma/client": "^6.11.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "effect": "^3.16.11",
    "framer-motion": "^12.23.0",
    "i18next": "^25.3.0",
    "next": "15.3.4",
    "next-i18next": "^15.4.2",
    "postcss": "^8.5.6",
    "prisma": "^6.11.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.5.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.0",
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3",
    "@luma-dev/eslint-plugin-luma-ts": "^1.0.1",
    "@storybook/addon-docs": "^8.4.0",
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-interactions": "^8.4.0",
    "@storybook/addon-links": "^8.4.0",
    "@storybook/addon-onboarding": "^8.4.0",
    "@storybook/blocks": "^8.4.0",
    "@storybook/nextjs": "^8.4.0",
    "@storybook/react": "^8.4.0",
    "@storybook/test": "^8.4.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-codegen": "^0.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-storybook": "^0.11.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "storybook": "^8.4.0",
    "typescript": "^5",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  }
}
